{
  "name": "vega-embed",
  "version": "6.11.1",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [
    {
      "name": "Dominik Moritz",
      "url": "https://www.domoritz.de"
    }
  ],
  "license": "BSD-3-Clause",
  "main": "build/vega-embed.js",
  "module": "build/vega-embed.module.js",
  "unpkg": "build/vega-embed.min.js",
  "jsdelivr": "build/vega-embed.min.js",
  "types": "build/src/embed.d.ts",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@types/semver": "^7.3.3",
    "@wessberg/rollup-plugin-ts": "^1.3.3",
    "babel-jest": "^26.3.0",
    "browser-sync": "^2.26.12",
    "concurrently": "^5.3.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.2.0",
    "node-sass": "^4.14.1",
    "rollup": "^2.26.6",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "terser": "^5.2.1",
    "typescript": "^4.0.2",
    "vega": "^5.10.0",
    "vega-lite": "^4.7.0",
    "vega-lite-dev-config": "^0.12.0"
  },
  "peerDependencies": {
    "vega": "^5.13.0",
    "vega-lite": "*"
  },
  "dependencies": {
    "fast-json-patch": "^3.0.0-1",
    "json-stringify-pretty-compact": "^2.0.0",
    "semver": "^7.3.2",
    "vega-schema-url-parser": "^2.1.0",
    "vega-themes": "^2.8.5",
    "vega-tooltip": "^0.23.2"
  },
  "scripts": {
    "prebuild": "rimraf build && rimraf build-es5 && yarn build:style",
    "build": "rollup -c",
    "build:style": "./build-style.sh",
    "clean": "rimraf build && rimraf src/style.ts && mkdir build",
    "prepublishOnly": "yarn clean && yarn build",
    "preversion": "yarn lint && yarn test",
    "serve": "browser-sync start --directory -s -f build *.html",
    "start": "yarn build && concurrently --kill-others -n Server,Rollup 'yarn serve' 'rollup -c -w'",
    "pretest": "yarn build:style",
    "test": "jest",
    "test:inspect": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "prepare": "beemo create-config",
    "prettierbase": "beemo prettier '*.{css,html}'",
    "eslintbase": "beemo eslint",
    "format": "yarn eslintbase --fix && yarn prettierbase --write",
    "lint": "yarn eslintbase && yarn prettierbase --check"
  },
  "beemo": {
    "module": "vega-lite-dev-config",
    "drivers": [
      "prettier",
      "eslint"
    ],
    "prettier": {
      "ignore": [
        "src/style.ts",
        "README.md"
      ]
    }
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
