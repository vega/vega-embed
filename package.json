{
  "name": "vega-embed",
  "version": "3.0.0-beta.19",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [{
    "name": "Jeffrey Heer",
    "url": "https://homes.cs.washington.edu/~jheer/"
  }, {
    "name": "Arvind Satyanarayan",
    "url": "http://arvindsatya.com"
  }, {
    "name": "Younghoon Kim"
  }, {
    "name": "Dominik Moritz",
    "url": "https://www.domoritz.de"
  },
    "Yuri Astrakhan <YuriAstrakhan@gmail.com>"
  ],
  "license": "BSD-3-Clause",
  "main": "src/embed.js",
  "devDependencies": {
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "eslint": "^3.19.0",
    "uglify-es": "^3.0.15",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "vega": "^3.0.0-beta.38",
    "vega-lite": "^2.0.0-beta.4",
    "d3-selection": "^1.1.0",
    "vega-schema-url-parser": "^1.0.0-beta.2"
  },
  "scripts": {
    "deploy": "npm run lint && scripts/deploy.sh",
    "lint": "eslint src/",
    "build": "browserify src/embed.js -t browserify-shim -d -s vega.embed > vega-embed.js",
    "postbuild": "uglifyjs vega-embed.js -cm > vega-embed.min.js",
    "watch": "watchify src/embed.js -t browserify-shim -v -d -s vega.embed -o vega-embed.js"
  },
  "browserify-shim": {
    "vega": "global:vega",
    "vega-lite": "global:vl"
  }
}
