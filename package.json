{
  "name": "vega-embed",
  "version": "3.5.2",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [
    {
      "name": "Jeffrey Heer",
      "url": "https://homes.cs.washington.edu/~jheer/"
    },
    {
      "name": "Dominik Moritz",
      "url": "https://www.domoritz.de"
    },
    {
      "name": "Arvind Satyanarayan",
      "url": "http://arvindsatya.com"
    },
    {
      "name": "Younghoon Kim"
    },
    {
      "name": "Yuri Astrakhan"
    }
  ],
  "license": "BSD-3-Clause",
  "main": "build/embed.js",
  "module": "build/index.js",
  "unpkg": "build/vega-embed.min.js",
  "jsdelivr": "build/vega-embed.min.js",
  "types": "build/embed.d.ts",
  "devDependencies": {
    "@types/d3-selection": "^1.3.0",
    "@types/semver": "^5.5.0",
    "browser-sync": "^2.23.7",
    "concurrently": "^3.5.1",
    "prettier": "^1.12.1",
    "rollup": "^0.58.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "d3-selection": "^1.3.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "semver": "^5.5.0",
    "vega-lib": "^3.3.1",
    "vega-lite": "^2.3.1",
    "vega-schema-url-parser": "^1.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf build && mkdir build",
    "deploy": "npm run lint && scripts/deploy.sh",
    "lint": "tslint -p .",
    "postbuild": "uglifyjs build/vega-embed.js -cm > build/vega-embed.min.js",
    "prebuild": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "serve": "browser-sync start --directory -s -f build *.html",
    "start": "concurrently --kill-others -n Server,Typescript,Rollup 'npm run serve' 'tsc -w' 'rollup -c -w'"
  }
}
