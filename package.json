{
  "name": "vega-embed",
  "version": "3.0.0-rc1",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [
    {
      "name": "Jeffrey Heer",
      "url": "https://homes.cs.washington.edu/~jheer/"
    },
    {
      "name": "Arvind Satyanarayan",
      "url": "http://arvindsatya.com"
    },
    {
      "name": "Younghoon Kim"
    },
    {
      "name": "Dominik Moritz",
      "url": "https://www.domoritz.de"
    },
    {
      "name": "Yuri Astrakhan"
    }
  ],
  "license": "BSD-3-Clause",
  "main": "src/embed.js",
  "types": "typings/vega-embed.d.ts",
  "devDependencies": {
    "@types/d3-selection": "^1.1.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "eslint": "^4.9.0",
    "tsify": "^3.0.3",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "uglify-es": "^3.1.3",
    "vega": "^3.0.0",
    "vega-lite": "^2.0.0-rc4",
    "watchify": "^3.9.0"
  },
  "peerDependencies": {
    "vega": "^3.0.0",
    "vega-lite": "^2.0.0-rc4"
  },
  "dependencies": {
    "compare-versions": "^3.1.0",
    "d3-selection": "^1.1.0",
    "vega-schema-url-parser": "^1.0.0-beta.3"
  },
  "scripts": {
    "deploy": "npm run lint && scripts/deploy.sh",
    "build": "browserify src/embed.ts -t browserify-shim -p [tsify -p site] -d -s vegaEmbed > build/vega-embed.js",
    "postbuild": "uglifyjs build/vega-embed.js -cm > build/vega-embed.min.js",
    "watch": "watchify src/embed.ts -t browserify-shim -p [tsify -p site] -v -d -s vegaEmbed -o build/vega-embed.js",
    "tsc": "tsc",
    "lint": "tslint -p ."
  },
  "browserify-shim": {
    "vega": "global:vega",
    "vega-lite": "global:vl"
  }
}
