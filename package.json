{
  "name": "vega-embed",
  "version": "3.11.0",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [
    {
      "name": "Jeffrey Heer",
      "url": "https://homes.cs.washington.edu/~jheer/"
    },
    {
      "name": "Dominik Moritz",
      "url": "https://www.domoritz.de"
    },
    {
      "name": "Arvind Satyanarayan",
      "url": "http://arvindsatya.com"
    },
    {
      "name": "Younghoon Kim"
    },
    {
      "name": "Yuri Astrakhan"
    }
  ],
  "license": "BSD-3-Clause",
  "main": "build/vega-embed.js",
  "module": "build/embed.js",
  "unpkg": "build/vega-embed.min.js",
  "jsdelivr": "build/vega-embed.min.js",
  "types": "build/embed.d.ts",
  "devDependencies": {
    "@types/d3-selection": "^1.3.0",
    "@types/jest": "^22.2.3",
    "@types/semver": "^5.5.0",
    "browser-sync": "^2.24.4",
    "concurrently": "^3.5.1",
    "jest": "^22.4.4",
    "jest-canvas-mock": "^1.0.2",
    "jest-css-modules": "^1.1.0",
    "prettier": "^1.12.1",
    "rollup": "^0.59.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "d3-selection": "^1.3.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "semver": "^5.5.0",
    "vega-lib": "^4.0.0-rc.1",
    "vega-lite": "^2.4.3",
    "vega-schema-url-parser": "^1.0.0",
    "vega-themes": "^2.0.0",
    "vega-tooltip": "^0.10.1"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf build && mkdir build",
    "format": "tslint -p . --fix && prettier --write '{src,test}/**/*.ts'",
    "lint": "tslint -p . && prettier --list-different '{src,test}/**/*.ts'",
    "postbuild": "uglifyjs build/vega-embed.js -cm > build/vega-embed.min.js",
    "prebuild": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "serve": "browser-sync start --directory -s -f build *.html",
    "start": "npm run build && concurrently --kill-others -n Server,Typescript,Rollup 'npm run serve' 'tsc -w' 'rollup -c -w'",
    "test": "jest",
    "test:inspect": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "watch:test": "jest --watch"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "<rootDir>/build",
      "src"
    ]
  }
}
